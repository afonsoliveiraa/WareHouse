<%= turbo_stream_from "units" %>

<main>

  <div class="header mb-5">
    <p class="fs-2">Unidades</p>
    <%= link_to "New Unidade", new_unit_path, data: { turbo_frame: dom_id(Unit.new) } %>
  </div>

  

  <div class="row">
    <div class="col-md-12 mx-auto">

      <%= turbo_frame_tag Unit.new %>
      <%= turbo_frame_tag "units" do %>
        <%= render @units %>
      <% end %>
      
    </div>
  </div>
  
</main>

